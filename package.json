{
  "name": "tournamentfileparser",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "node src/server.ts",
    "dev-ts-node": "nodemon --exec ts-node src/server.ts",
    "copy:views": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "copy:public": "copyfiles -u 0 \"public/**/*\" dist/",
    "build": "tsc && npm run copy:views && npm run copy:public",
    "start": "node dist/server.js"
  },
  
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "engines": { "node": ">=18" }
}
